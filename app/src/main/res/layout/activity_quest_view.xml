<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/light_black"
        tools:context=".Quest_view">

        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="353dp"
            android:layout_height="match_parent"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/custom_layout"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="10dp"
                android:layout_marginBottom="603dp"
                android:fontFamily="@font/open_sans"
                android:text="Altura"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/editAltura"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="14dp"
                android:background="@drawable/custom_textview"
                android:hint="Respuesta 1"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="11dp"
                android:layout_marginTop="64dp"
                android:layout_marginBottom="522dp"
                android:fontFamily="@font/open_sans"
                android:text="Peso"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@+id/editAltura" />

            <EditText
                android:id="@+id/editPeso"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="139dp"
                android:layout_marginEnd="17dp"
                android:background="@drawable/custom_textview"
                android:hint="Respuesta 2"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="11dp"
                android:layout_marginTop="64dp"
                android:layout_marginBottom="446dp"
                android:fontFamily="@font/open_sans"
                android:text="Edad"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/editEdad"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="215dp"
                android:layout_marginEnd="13dp"
                android:background="@drawable/custom_textview"
                android:hint="Respuesta 3"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textSexo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="64dp"
                android:layout_marginBottom="367dp"
                android:fontFamily="@font/open_sans"
                android:text="Sexo"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <RelativeLayout
                android:id="@+id/relativeLayout"
                android:layout_width="500dp"
                android:layout_height="350dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="-1dp"
                android:layout_marginBottom="14dp"
                app:layout_constraintTop_toBottomOf="@+id/textSexo">

                <RadioButton
                    android:id="@+id/radioChico"
                    android:layout_width="78dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="35dp"
                    android:layout_marginBottom="10dp"
                    android:text="Chico"
                    tools:ignore="MissingConstraints" />

                <ImageView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginTop="42dp"
                    android:layout_marginBottom="158dp"
                    android:src="@drawable/male_chad" />

                <RadioButton
                    android:id="@+id/radioChica"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="239dp"
                    android:layout_marginLeft="35dp"
                    android:layout_marginEnd="25dp"
                    android:layout_marginBottom="10dp"
                    android:text="Chica"
                    tools:ignore="MissingConstraints" />

                <ImageView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="231dp"
                    android:layout_marginTop="44dp"
                    android:layout_marginEnd="-24dp"
                    android:layout_marginBottom="156dp"
                    android:src="@drawable/female" />

                <Button
                    android:id="@+id/ButtonSave"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="138dp"
                    android:layout_marginEnd="128dp"
                    android:layout_marginBottom="66dp"
                    android:background="@drawable/custom_button"
                    android:text="Save"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:ignore="MissingConstraints" />


            </RelativeLayout>


        </RelativeLayout>


    </android.support.constraint.ConstraintLayout>
</layout>